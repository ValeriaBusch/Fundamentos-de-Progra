#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

void main() {
	int nota1, nota2;
	cout<<"Ingrese su primer nota: ";
	cin>>nota1;
	cout<<"Ingrese su segunda nota: ";
	cin>>nota2;
	nota2= nota1+nota2;
	cout<<"Ingrese su tercer nota: ";
	cin>>nota1;
	nota1 = nota1+nota2;
	cout<<"Ingrese su cuarta nota: ";
	cin>>nota2;
	nota2 = nota1+nota2;
	cout<<"Ingrese su quinta nota: ";
	cin>>nota1;
	nota1 = (nota1 + nota2)/5;
	
	cout<<"Su promedio es: "<<nota1;
	getch();
}



Proceso Promedio
	Escribir "Ingrese su primer nota:"
	Leer X
	Escribir "Ingrese su segunda nota:"
	Leer X2
	Escribir "Ingrese su tercera nota:"
	Leer X3
	Escribir "Ingrese su cuarta nota:"
	Leer X4
	Escribir "Ingrese su quinta nota:"
	Leer X5
	P<-(X+X2+X3+X4+X5)/5
	Escribir "Su promedio es:"  ,P
FinProceso

